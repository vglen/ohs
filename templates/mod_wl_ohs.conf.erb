#      SecureProxy ON
#      WLProxySSL ON
#      RequestHeader set WL-Proxy-SSL true
#      WlSSLWallet <%= @ohsint_base %>/<%= @name %>/config/OHS/OHS1/wallet
</IfModule>

<Location /<%= @wl_url %>>
      SetHandler weblogic-handler
